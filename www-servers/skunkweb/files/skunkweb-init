# $Id$

DAEMON_CMD=/usr/bin/swmgr
opts="${opts} configtest"

configtest() {
	${DAEMON_CMD} configtest
}
start() {
	ebegin "Starting skunkweb"
	${DAEMON_CMD} start > /dev/null 2>&1
	eend $?
}
stop() {
	ebegin "Stopping skunkweb"
	${DAEMON_CMD} stop > /dev/null 2>&1
	eend $?
}
